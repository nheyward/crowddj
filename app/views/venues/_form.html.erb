<%= form_for(@venue, :html => { :multipart => true }) do |f| %>
    
	<%= render 'shared/error_messages', :object => f.object %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :postcode %><br />
    <%= f.text_field :postcode %>
  </div>
  <div class="field">
    <%= f.label :website %><br />
    <%= f.text_field :website %>
  </div>
  <div class="field">
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </div>
  <div class="field">
	<%= image_tag @venue.image.url, :size=> "150x150", :alt => "Venue image", :class => "round" %><br />
 	<%= f.label :image, 'Change Image' %>
	<%= f.file_field :image %>
  </div>
<br />
  <div class="actions">
    <%= f.submit "Go"%>
  </div>
<% end %>
